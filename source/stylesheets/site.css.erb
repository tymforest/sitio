}
.acordion-cerrado {
  max-height: 0 !important;
  -webkit-transition: max-height 0.35s cubic-bezier(0.45, 0.25, 0.25, 1);
  -moz-transition: max-height 0.35s cubic-bezier(0.45, 0.25, 0.25, 1);
  -o-transition: max-height 0.35s cubic-bezier(0.45, 0.25, 0.25, 1);
  transition: max-height 0.35s cubic-bezier(0.45, 0.25, 0.25, 1);
}
.acordion-abierto {
  -webkit-transition: max-height 0.50s cubic-bezier(0.45, 0.25, 0.25, 1);
  -moz-transition: max-height 0.50s cubic-bezier(0.45, 0.25, 0.25, 1);
