/* Botón de Contacto Móvil - - - - - - - - - -  */
#contacto-movil {
  position: fixed;
  z-index: 2;
  right: 10px;
  bottom: 10px;
  width: 50px;
  height: 50px;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
}
#contacto-movil.email, #contacto-movil.telefono {
  background-repeat: no-repeat;
  background-position: center;
  -webkit-box-shadow: 0 0 10px 0 rgba(0,0,0,0.25);
  -moz-box-shadow: 0 0 10px 0 rgba(0,0,0,0.25);
  box-shadow: 0 0 10px 0 rgba(0,0,0,0.25);
}
#contacto-movil.telefono {
  background-image: url('../images/iconos/tel.svg');
  background-size: 20px 20px, cover;
  background-color: #25c315;
  -ms-transform: rotate(15deg);
  -webkit-transform: rotate(15deg);
  transform: rotate(15deg);
}
#contacto-movil.email {
  background-image: url('../images/iconos/email.svg');
  background-size: 24px 24px, cover;
  background-color: #0080ff;
}
.esconder {
  opacity: 0;
  pointer-events: none;
}
.mostrar {
  pointer-events: auto;
  cursor: pointer;
}
@-webkit-keyframes mostrar-boton {
	0% { opacity: 0; transform: scale(0.5); }
  100% { opacity: 1; transform: scale(1); }
}
@-moz-keyframes mostrar-boton {
	0% { opacity: 0; transform: scale(0.5); }
  100% { opacity: 1; transform: scale(1); }
}
@-o-keyframes mostrar-boton {
	0% { opacity: 0; transform: scale(0.5); }
  100% { opacity: 1; transform: scale(1); }
}
@keyframes mostrar-boton {
	0% { opacity: 0; transform: scale(0.5); }
  100% { opacity: 1; transform: scale(1); }
}
@-webkit-keyframes esconder-boton {
	0% { opacity: 1; transform: scale(1); }
  100% { opacity: 0; transform: scale(0.5); }
}
@-moz-keyframes esconder-boton {
	0% { opacity: 1; transform: scale(1); }
  100% { opacity: 0; transform: scale(0.5); }
}
@-o-keyframes esconder-boton {
	0% { opacity: 1; transform: scale(1); }
  100% { opacity: 0; transform: scale(0.5); }
}
@keyframes esconder-boton {
	0% { opacity: 1; transform: scale(1); }
  100% { opacity: 0; transform: scale(0.5); }
}
.mostrar-boton {
	-webkit-animation: mostrar-boton 0.30s cubic-bezier(0.45, 0.25, 0.25, 1) both;
	-moz-animation: mostrar-boton 0.30s cubic-bezier(0.45, 0.25, 0.25, 1) both;
	-o-animation: mostrar-boton 0.30s cubic-bezier(0.45, 0.25, 0.25, 1) both;
	animation: mostrar-boton 0.30s cubic-bezier(0.45, 0.25, 0.25, 1) both;
}
.esconder-boton {
	-webkit-animation: esconder-boton 0.30s cubic-bezier(0.45, 0.25, 0.25, 1) both;
	-moz-animation: esconder-boton 0.30s cubic-bezier(0.45, 0.25, 0.25, 1) both;
	-o-animation: esconder-boton 0.30s cubic-bezier(0.45, 0.25, 0.25, 1) both;
	animation: esconder-boton 0.30s cubic-bezier(0.45, 0.25, 0.25, 1) both;
}
.delatar {
  -webkit-animation-delay: 0.40s;
  -moz-animation-delay: 0.40s;
  -o-animation-delay: 0.40s;
  animation-delay: 0.15s;
}
.delatar-2x {
  -webkit-animation-delay: 0.30s;
  -moz-animation-delay: 0.30s;
  -o-animation-delay: 0.30s;
  animation-delay: 0.30s;
}
