      text-transform: none;
      text-decoration: none;
      color: #969696;
    }
    #menu-productos ul li a:hover, #product-nav ul ul li a:hover {
      color: #25c315;
    }
    .boton-acordion {
      right: 0;
      width: 8px; height: 46px;
    }
    .abrir-acordion, .cerrar-acordion {
      width: 8px; height: 8px;
    }
    /*
      ----------------------------------------
      Barra de Anuncios
      ----------------------------------------
    */
    .barra-anuncios {
      display: block;
      width: 100%;
      margin: 60px 0 0 0;
      text-align: center;
      border-bottom: 1px solid #eaeaea;
      background-color: #fbfbfb;
    }
    .barra-carrusel {
      width: 100%;
      height: 100%;
      position: relative;
      overflow-x: hidden;
      overflow-y: hidden;
      white-space: nowrap;
      text-align: center;
    }
    .anuncio {
      display: inline-block;
      width: 100%;
      margin: 0 auto;
      padding: 1.0rem 0;
    }
    .anuncio-barra h1 {
      font-family: 'Open Sans', sans-serif;
      font-size: 2.0rem;
      margin: 0 0 1.0rem 0;
    }
    .anuncio-barra h4, .anuncio-barra p, .anuncio-barra a {
      display: inline-block;
      font-family: 'Open Sans', sans-serif;
      color: #212121;
    }
    .anuncio-barra h4 {
      padding: 0 3px 0 0;
      font-weight: 600;
      font-size: 1.0rem;
    }
    .anunci-barra p {
      margin: 0 0 1.0rem 0;
      font-size: 1.0rem;
      color: #969696;
    }
    .anuncio-barra a {
      font-size: 0.75rem;
      text-decoration: underline;
      color: black;
    }
    .anuncio a {
      -webkit-transition: color 250ms ease-out;
      -moz-transition: color 250ms ease-out;
      -o-transition: color 250ms ease-out;
      transition: color 250ms ease-out;
    }
    .anuncio a:hover {
      color: #25c315;
    }
    .padding-enlace {
      padding: 0 3px 0 0;
    }
    /*
      ----------------------------------------
      Inicio
      ----------------------------------------
    */
    section.inicio {
      padding-top: 0;
    }
    section.inicio:first-of-type {
      border-bottom: 1px solid #eaeaea;
    }
    section.inicio.inicial header {
      margin-bottom: 15px;
    }
    .aplicacion-anuncio {
      height: 250px;
      margin-bottom: 10px;
    }
    .contenedor-movil-carrusel .tarjeta-producto header {
      margin-bottom: 0;
    }
    .inicio .contenedor-movil-carrusel .tarjeta-servicio {
      width: 33%;
      padding: 30px;
    }
    .reticula.tablet-inicio .tarjeta-producto {
      margin-bottom: 0;
    }
    .producto-anuncio {
      height: 250px;
    }
    #google .reticula {
      position: relative;
    }
    .producto-anuncio {
      margin-bottom: 10px;
    }
    .lista-de-sucursales {
      transform: translateX(0);
      transition: transform 0.50s cubic-bezier(0.45, 0.25, 0.25, 1);
    }
    .tarjeta-sucursal {
      min-width: 32%;
      max-width: 32%;
      -webkit-transition: background-color 350ms ease-in-out;
      -moz-transition: background-color 350ms ease-in-out;
      -o-transition: background-color 350ms ease-in-out;
      transition: background-color 350ms ease-in-out;
    }
    .tarjeta-sucursal:hover {
      background-color: #fafafa;
    }
    .tarjeta-sucursal:first-of-type {
      margin-left: 0;
    }
    .sucursal-siguiente, .sucursal-previa {
      display: inline-block;
      position: absolute;
      top: 45%;
      z-index: 2;
      height: 57px; width: 57px;
      cursor: pointer;
      background-position: center;
      background-size: 25px;
      background-repeat: no-repeat;
      background-image: url('../images/iconos/flecha-carrusel.svg');
    }
    .no-js .sucursal-siguiente, .no-js .sucursal-previa {
      display: none;
    }
    .sucursal-previa {
      left: 5px;
    }
    .sucursal-siguiente {
      right: 5px;
      transform: rotate(180deg);
    }
    .sucursal-siguiente.inactiva, .sucursal-previa.inactiva {
      pointer-events: none;
      background-image: url('../images/iconos/flecha-carrusel-inactiva.svg');
    }
    /*
      ----------------------------------------
      Galería
      ----------------------------------------
    */
    .aplicacion-anuncio h2, .producto-anuncio h2 {
      margin: 0;
      font-size: 1.45rem;
      letter-spacing: 0.15rem;
      color: white;
      text-align: center;
      text-shadow: 1px 2px rgba(0,0,0,0.55);
    }
    /* Cubiertas - - - - - - - - - - */
    .js .anuncio-cubierta {
      padding: 0 35%;
    }
    /* Detalle - - - - - - - - - - */
    .no-js .detalle-contenido {
      position: static;
      padding: 45% 0;
    }
    /*
      ----------------------------------------
      Detalle de Producto por Aplicación
      ----------------------------------------
    */
    section.anuncio {
      padding: 12.5px 0;
    }
    section.anuncio:first-of-type {
      padding-top: 50px;
    }
    section.anuncio:last-of-type {
      padding-bottom: 50px;
    }
    .aplicacion-anuncio, .producto-anuncio {
      overflow: hidden;
      position: relative;
    }
    .detalle-anuncio {
      display: flex;
      align-items: center;
      height: 350px;
      padding: 0;
    }
    /*
      ----------------------------------------
      Detalle de Lista de Productos
      ----------------------------------------
    */
    /*
      ----------------------------------------
      Lista de Categorías de Producto
      ----------------------------------------
    */
    .aplicacion-anuncio .cubierta, .producto-anuncio .cubierta {
      display: block;
      position: absolute;
      top: 0;
      left: 0;
      opacity: 0;
      width: 100%;
      height: 100%;
      background-color: #000000;
    }
    .tarjeta-producto .cubierta, .aplicacion-anuncio .cubierta, .producto-anuncio .cubierta {
      -webkit-transition: opacity 350ms ease-in-out;
      -moz-transition: opacity 350ms ease-in-out;
      -o-transition: opacity 350ms ease-in-out;
      transition: opacity 350ms ease-in-out;
    }
    .tarjeta-producto .cubierta:hover {
      opacity: 0.20;
    }
    .aplicacion-anuncio .cubierta:hover, .producto-anuncio .cubierta:hover {
      opacity: 0.50;
    }
    .tarjeta-producto header a {
      -webkit-transition: color 250ms ease-out;
      -moz-transition: color 250ms ease-out;
      -o-transition: color 250ms ease-out;
      transition: color 250ms ease-out;
    }
    .tarjeta-producto header a:hover {
      color: #f5a623;
    }
    /*
      ----------------------------------------
      Ofertas
      ----------------------------------------
    */
    section.lista.ofertas {
      margin: 0 0 50px;
      padding: 0 0 50px;
    }
    .enlace-imagen {
      background-color: #fafafa;
    }
    .enlace-imagen .prueba-hover {
      -webkit-transition: opacity 250ms ease-out 250ms, transform 250ms ease-out 250ms;
      -moz-transition: opacity 250ms ease-out 250ms, transform 250ms ease-out 250ms;
      -o-transition: opacity 250ms ease-out 250ms, transform 250ms ease-out 250ms;
      transition: opacity 250ms ease-out 250ms, transform 250ms ease-out 250ms;
    }
    .enlace-imagen:hover .prueba-hover {
      opacity: 0;
      -ms-transform: translate(0, 5px);
      -webkit-transform: translate(0, 5px);
      transform: translate(0, 5px);
      -webkit-transition: opacity 250ms ease-out, transform 250ms ease-out;
      -moz-transition: opacity 250ms ease-out, transform 250ms ease-out;
      -o-transition: opacity 250ms ease-out, transform 250ms ease-out;
      transition: opacity 250ms ease-out, transform 250ms ease-out;
    }
    .enlace-imagen {
      overflow: auto;
    }
    .enlace-imagen::-webkit-scrollbar {
      display: none;
      overflow: -moz-scrollbars-none;
    }
    .tarjeta-ofertas {
      display: block;
      position: absolute;
      opacity: 0;
      width: 100%;
      height: 100%;
      padding: 40px 20px;
      -ms-transform: translate(0, 2.5px);
     	-webkit-transform: translate(0, 2.5px);
      transform: translate(0, 2.5px);
      -webkit-transition: opacity 250ms ease-out, transform 250ms ease-out;
      -moz-transition: opacity 250ms ease-out, transform 250ms ease-out;
      -o-transition: opacity 250ms ease-out, transform 250ms ease-out;
      transition: opacity 250ms ease-out, transform 250ms ease-out;
    }
    .ofertas h3 {
      margin: 0 0 5px 0;
    }
    .tarjeta-ofertas h2 {
      margin: 0 0 15px 0;
      padding: 0 0 15px 0;
      color: #212121;
      border-bottom: 1px solid #eaeaea;
    }
    .tarjeta-ofertas p {
      text-align: right;
    }
    .enlace-imagen:hover .tarjeta-ofertas {
      opacity: 1;
      -ms-transform: translate(0, 0);
      -webkit-transform: translate(0, 0);
      transform: translate(0, 0);
      -webkit-transition: opacity 250ms ease-out 250ms, transform 250ms ease-out 250ms;
      -moz-transition: opacity 250ms ease-out 250ms, transform 250ms ease-out 250ms;
      -o-transition: opacity 250ms ease-out 250ms, transform 250ms ease-out 250ms;
      transition: opacity 250ms ease-out 250ms, transform 250ms ease-out 250ms;
    }
    /*
      ----------------------------------------
      Detalle de Subcategoría
      ----------------------------------------
    */
    .descripcion.subcategoria {
      margin: 0;
      padding: 0;
      border-bottom: 0;
    }
    /*
      ----------------------------------------
      Detalle de Producto
      ----------------------------------------
    */
    #encabezado-de-producto {
      border-bottom: none;
    }
    #encabezado-de-producto .encabezado {
      margin: 0;
    }
    .precio-grande {
      font-size: 1.5rem;
    }
    .precio-grande span {
      font-size: 0.70rem;
    }
    h1 span.oferta-detalle {
      top: -7px;
      font-size: 0.35rem;
    }
    /*
      ----------------------------------------
      Página en Construcción
      ----------------------------------------
    */
    .en-construccion .descripcion {
      max-width: 65%;
    }
    /*
      ----------------------------------------
      Tarjeta de Servicio
      ----------------------------------------
    */
    .tarjeta-servicio {
      margin: 0 0 15px 0;
      padding: 35px;
    }
    .tarjeta-servicio h5 {
      margin-bottom: 0.5rem;
    }
    .servicios .contenedor-tablet-carrusel .tarjeta-servicio {
      max-width: 100%;
      border: none;
    }
    /*
      ----------------------------------------
      Servicios
      ----------------------------------------
    */
    section.servicios {
      padding-bottom: 0;
    }
    .contenedor-desktop.servicios {
      padding-top: 50px;
      padding-bottom: 50px;
    }
    .contenedor-desktop.servicios #servicios-relacionados {
      margin-left: 50px;
      padding: 0;
    }
    .servicios .contenedor-movil-carrusel .tarjeta-servicio {
      width: 33%;
      padding: 50px;
      margin: 0;
    }
    .estado.sucursal-detalle {
      width: 100%;
      margin: 0;
      text-align: left;
    }
    /*
      ----------------------------------------
      Sucursales
      ----------------------------------------
    */
    .sucursales h1 {
      margin: 0 0 1.0rem 0;
    }
    section.sucursales {
      padding: 100px 0;
    }
    .sucursales header {
      border-bottom: none;
    }
    .sucursales header p {
      max-width: 100%;
    }
    section.detalle.sucursal {
      margin: 125px 0 100px 0;
    }
    .tarjeta-detalle-sucursal {
      height: 600px;
      padding: 50px 50px 0 50px;
    }
    img.sucursal {
      width: auto;
      height: 600px;
    }
    .js #enlace-mapa:hover {
      display: none;
      cursor: pointer;
    }
    /*
      ----------------------------------------
      Aviso de Privacidad
      ----------------------------------------
    */
    header.aviso {
      margin: 0 0 50px 0;
    }
    .layout-texto {
      margin-top: 120px;
    }
    /*
      ----------------------------------------
      Nosotros
      ----------------------------------------
    */
    section.nosotros {
      padding: 100px 0;
    }
    .tarjeta-nosotros.contenido {
      margin: 0;
      padding: 0;
    }
    /*
      ----------------------------------------
      Footer
      ----------------------------------------
    */
    footer {
      padding: 3.5rem 0;
    }
    footer .seccion, footer .subseccion {
      -webkit-transition: color 250ms ease-out;
      -moz-transition: color 250ms ease-out;
      -o-transition: color 250ms ease-out;
      transition: color 250ms ease-out;
    }
    footer .seccion:hover {
      font-weight: 400;
      color: #25c315;
    }
    footer .subseccion:hover {
      font-weight: 300;
      color: #25c315;
    }
    /*
      ----------------------------------------
      Botón de Contacto Móvil
      ----------------------------------------
    */
    #contacto-movil {
      display: none;
    }
    /*
      ----------------------------------------
      Error 404
      ----------------------------------------
    */
    .no-flexbox section.error {
      margin-top: 20%;
    }
    section.error p {
      max-width: 45%;
    }
  }
  @media screen and (min-width: 1120px) and (max-width: 1355px) {
    .contenedor-desktop-galeria {
      overflow: auto;
      max-width: 1120px;
      margin: 0 auto;
    }
  }
  .esconder-desktop {
    display: none;
  }
