  background-repeat: no-repeat;
}
.abrir-acordion {
  background-image: url('../images/iconos/abrir-acordion.svg');
}
.cerrar-acordion {
  -webkit-transition: all 0.15s ease-in;
  -moz-transition: all 0.15s ease-in;
  -o-transition: all 0.15s ease-in;
