/* Menu Principal - - - - - - - - - - */
.principal {
  position: fixed;
  top: 0;
  z-index: 2;
  width: 100%;
  height: 60px;
  text-align: center;
  background-color: white;
  border-bottom: 1px solid #eaeaea;
