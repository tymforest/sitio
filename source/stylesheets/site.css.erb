/*
  ========================================
  Global Configuration
  ========================================
*/
*, *:after, *:before {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
body {
  width: 100%;
  height: 100%;
  position: relative;
  z-index: 1;
}
main {
  padding-top: 60px;
}
main.store-detail {
  padding-top: 0;
}
.lista-movil {
  margin: 0;
}
article {
  margin: 0 0 50px 0;
  padding: 0 0 50px 0;
  border-bottom: 1px solid #eaeaea;
}
article:last-of-type {
  border-bottom: none;
}
.reticula:after {
  content: "";
  display: table;
  clear: both;
}
.barra-anuncios {
  display: none;
}
.difuminar {
  opacity: 0;
  -webkit-transition: opacity 0.35s ease-in-out 0.10s;
  -moz-transition: opacity 0.35s ease-in-out 0.10s;
  -o-transition: opacity 0.35s ease-in-out 0.10s;
  transition: opacity 0.35s ease-in-out 0.10s;
}
.difuminando {
  opacity: 1;
}
.espacio-movil {
  margin-bottom: 25px;
}
.altura-100 {
  height: 100%;
}
div[itemtype="http://schema.org/LocalBusiness"] {
  display: none;
}
/*
  ========================================
  Estilos Globales
  ========================================
*/
/*
  ----------------------------------------
  Texto y Spans
  ----------------------------------------
*/
h1 {
  margin: 0 0 0.45rem 0;
  font-family: 'Open Sans', sans-serif;
  font-weight: 400;
  font-size: 1.5rem;
  line-height: 2.0rem;
  color: #212121;
}
h1 span {
  margin: 0 0 0 0.35rem;
  font-family: 'Open Sans', sans-serif;
  font-weight: 400;
  font-size: 0.5em;
  color: #969696;
}
h1.titulo-detalle {
  margin: 0 0 2.0rem 0;
}
h1.ofertas {
  margin: 0;
}
h2 {
  margin: 0 0 0.75rem 0;
  font-family: 'Open Sans', sans-serif;
  font-weight: 600;
  font-size: 0.75rem;
  letter-spacing: 0.1rem;
  text-transform: uppercase;
}
h3 {
  margin: 0 0 5px 0;
  font-family: 'Open Sans', sans-serif;
  font-weight: 600;
  font-size: 0.75rem;
  letter-spacing: 0.1rem;
  line-height: 1.15rem;
  text-transform: uppercase;
}
h3 span, p.iva {
  font-style: italic;
  font-weight: 300;
  font-size: 0.70rem;
  letter-spacing: 0;
  text-transform: none;
  color: #b3b3b3;
}
h3 span {
  margin: 0 0 0 10px;
}
h4 {
  font-family: 'Open Sans', sans-serif;
  font-weight: 400;
  font-size: 1.25rem;
  line-height: 1.85rem;
  letter-spacing: 0.075rem;
}
h5 {
  margin: 0 0 0.25rem 0;
  font-family: 'Open Sans', sans-serif;
  font-weight: 400;
  font-size: 1.0rem;
  letter-spacing: 0.025rem;
  line-height: 1.65rem;
  color: #212121;
}
h5 span {
  margin: 0 0 0 25px;
  /*color: #f5a623;*/
}
h6 {
  margin: 0 0 0.35rem 0;
  font-family: 'Open Sans', sans-serif;
  font-weight: 600;
  font-size: 0.75rem;
  letter-spacing: 0.1rem;
  text-transform: uppercase;
  color: #b3b3b3;
}
a, p {
  font-family: 'Open Sans', sans-serif;
  font-weight: 300;
  font-size: 1.0rem;
  line-height: 1.85rem;
  letter-spacing: 0.02rem;
}
a {
  line-height: 0;
  text-decoration: none;
  color: #25c315;
}
p {
  color: #969696;
}
p .unidad, h5 .unidad {
  display: inline-block;
  margin-left: 2.5px;
  font-weight: 400;
  font-size: 0.65em;
  letter-spacing: 0.1em;
  text-transform: uppercase;
}
.flecha {
  display: inline-block;
  position: relative;
  top: 0;
  width: 10px;
  height: 10px;
  margin-left: 5px;
  background-image: url('../images/iconos/flecha.svg');
  background-size: contain;
  background-repeat: no-repeat;
  background-position: center;
}
.centrado {
  text-align: center;
}
.negrita {
  font-weight: 400;
}
h1 span.oferta-detalle:before {
  content: "En Oferta";
}
.tarjeta-producto header span.oferta-lista {
  position: relative;
  top: -5px;
  width: 5px;
  height: 5px;
  margin: 0 0 3px 0;
  padding: 0;
  border-radius: 50%;
  background-color: #f5a623;
}
h1 span.oferta-detalle {
  position: relative;
  top: -5px;
  margin: 0 0 0 0.5rem;
  padding: 0.20rem 0.30rem;
  font-family: 'Open Sans', sans-serif;
  font-weight: 400;
  font-size: 0.40em;
  letter-spacing: 0.05rem;
  border: 1px solid #f5a623;
  color: #f5a623;
}
/*
  ----------------------------------------
  Listas
  ----------------------------------------
*/
ul.puntos, ul.direcciones {
  display: block;
  padding: 1.0rem 0 0 1.0rem;
  list-style: none;
}
ul.puntos li {
  position: relative;
  margin-bottom: 0.75rem;
  padding-left: 1.15rem;
}
ul.puntos li:before {
  content: "◇";
  display: inline-block;
  position: absolute;
  top: 6px;
  left: 0;
  font-size: 0.75rem;
  color: #25c315;
}
ul.puntos li p, ul.direcciones li p {
  margin-bottom: 0;
  letter-spacing: 0.035rem;
  line-height: 1.5rem;
  font-size: 1.0rem;
  color: #969696;
}
ul.sucursales li {
  margin-bottom: 0.75rem;
}
ul.servicios li a {
  display: block;
  padding: 1.5rem 1.0rem;
  font-weight: 400;
}
/*
  ----------------------------------------
  Colores
  ----------------------------------------
*/
.obscuro {
  color: #212121;
}
.mediano {
  color: #969696;
}
.claro {
  color: #b3b3b3;
}
.blanco {
  color: white;
}
.naranja {
  color: #f5a623;
}
.verde {
  color: #25c315;
}
/*
  ----------------------------------------
  Bordes
  ----------------------------------------
*/
.borde-superior {
  border-top: 1px solid #eaeaea;
}
section.inicio.no-borde-inferior, section.inicio.inicial.no-borde-inferior {
  border: none;
}
section.anuncio.no-bordes {
  border-bottom: none;
}
section.inicio.inicial header.margen-servicios {
  margin-bottom: 1.5rem;
}
section.detalle.servicios-sucursal {
  border: none;
}
/*
  ----------------------------------------
  Menú
  ----------------------------------------
*/
.sombreado {
  /*box-shadow: 0 0 25px rgba(0,0,0,0.10);*/
}
/*
  ----------------------------------------
  Estados de formas de Contacto
  ----------------------------------------
*/
.estado {
  display: inline-block;
  margin: 0 0.5rem 0 0;
  font-family: 'Open Sans', sans-serif;
  font-weight: 300;
  font-size: 0.875rem;
  letter-spacing: 0.03rem;
  color: #b3b3b3;
}
.abierto span, .cerrado span, .en-la-comida span {
  display: inline-block;
  position: relative;
  top: -3px;
  width: 5px;
  height: 5px;
  margin: 0 3px 0 0;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
}
.abierto span {
  background-color: #25c315;
}
.cerrado span {
  background-color: #d0021b;
}
.en-la-comida span {
  background-color: #f5a623;
}
/*
  ----------------------------------------
  Botones
  ----------------------------------------
*/
.boton {
  display: inline-block;
  padding: 0.40rem 0.65rem 0.60rem;
  font-family: 'Open Sans', sans-serif;
  font-weight: 400;
  font-size: 0.875rem;
  letter-spacing: 0.05rem;
  text-decoration: none;
  color: white;
  border: none;
}
.boton.accion {
  padding: 1.0rem;
  text-align: center;
  background-color: #25c315;
}
.boton.telefono {
  background-color: #25c315;
}
.boton.email {
  background-color: #0080ff;
}
.telefono span, .email span {
  display: inline-block;
  position: relative;
  top: 2px;
  margin: 0 10px 0 0;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
}
.telefono span {
  width: 15px; height: 15px;
  background-image: url('../images/iconos/tel.svg');
}
.email span {
  width: 18px; height: 16px;
  background-image: url('../images/iconos/email.svg');
}
/*
  ----------------------------------------
  Secciones
  ----------------------------------------
*/
section {
  overflow: hidden;
}
section.inicio header {
  padding: 35px 0 0px;
}
section.inicio header > .contenido {
  padding: 0;
}
section.inicio, section.detalle {
  padding: 0 0 15px 0;
  border-bottom: 1px solid #eaeaea;
}
section.detalle.producto {
  padding: 0;
}
section.inicio header > .contenido.carrusel {
  padding: 0;
}
section.inicio:first-of-type, section.detalle:last-of-type {
  border-bottom: none;
}
section.anuncio {
  padding: 0 0 50px 0;
  /*border-bottom: 1px solid #eaeaea;*/
}
section.anuncio:last-of-type {
  border-bottom: none;
}
section.nosotros {
  padding: 100px 0;
}
section.servicios:first-of-type {
  padding: 50px 0;
}
section.detalle.sucursal header {
  padding: 50px 0;
  text-align: center;
}
section.detalle.sucursal header h1 {
  margin: 0;
}
section#accion  {
  padding: 5.0rem 0;
  border-top: 1px solid #eaeaea;
}
section.centrado, section.lista {
  text-align: center
}
section.lista {
  padding: 0 0 50px 0;
}
section.lista > header {
  padding: 25px 0;
}
section.lista header h1 {
  margin: 0;
}
section.lista.ofertas {
  margin: 0 0 25px;
  padding: 0 0 25px;
  border-bottom: 1px solid #eaeaea;
}
section.lista.ofertas:last-of-type {
  padding: 0 0 50px;
  border-bottom: none;
}
section.sucursales, section.empleos {
  padding: 0 0 50px 0;
}
section.empleos {
  border-bottom: 1px solid #eaeaea;
}
section.empleos:last-of-type {
  border-bottom: none;
}
.tarjeta-producto header {
  padding: 2.0rem 0;
}
.centrado header p {
  max-width: 75%;
  margin: 0 auto 1.15rem;
}
<%# Config %>
<% service_image = "" %>
<% data.site.index_page.each do |id, page| %>
  <% if page.services.image %>
    <% service_image = page.services.image.url %>
  <% end %>
<% end %>
<%# End %>
section#servicios {
  height: 350px;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  justify-content: center;
  background-image: url('<%= service_image %>');
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
}
section#servicios .contenido {
  padding: 0;
}
/*
  ----------------------------------------
  Imágenes
  ----------------------------------------
*/
img {
  width: 100%;
  max-width: 100%;
  vertical-align: middle;
}
.portada {
  position: relative;
  width: auto;
  height: 400px;
  text-align: center;
  background-color: #f8f8f8;
}
.flexbox .portada {
  display: -webkit-flex;
  display: flex;
  align-items: center;
  justify-content: center;
}
.no-flexbox .portada {
  position: relative;
}
#sucursales, #empleos, #nosotros, #servicios-portada {
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
}
.completo {
  margin: 0 0 3px 0;
}
.mitad-izquierda {
  padding: 0 1px 0 0;
}
.mitad-derecha {
  padding: 0 0 0 1px;
}
.mitad-izquierda.producto, .mitad-derecha.producto {
  display: block;
  padding: 0;
}
.mitad-izquierda.producto {
  padding-bottom: 1px;
}
/*
  ----------------------------------------
  Inicio
  ----------------------------------------
*/
.aplicacion-anuncio, .producto-anuncio {
  padding: 15px;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
}
.aplicacion-anuncio {
  height: 225px;
  margin-bottom: 2px;
}
.producto-anuncio {
  height: 175px;
  margin-bottom: 20px;
}
.no-flexbox .aplicacion-anuncio, .no-flexbox .producto-anuncio {
  display: block;
  position: relative;
}
.flexbox .aplicacion-anuncio, .flexbox .producto-anuncio {
  display: -webkit-flex;
  display: flex;
  align-items: center;
  justify-content: center;
}
.aplicacion-anuncio h2, .producto-anuncio h2 {
  position: relative;
  z-index: 1;
  margin: 0;
  font-size: 1.15rem;
  line-height: 1.35rem;
  letter-spacing: 0.15rem;
  text-transform: uppercase;
  color: white;
  text-align: center;
  text-shadow: 1px 1px rgba(0,0,0,0.25);
}
.no-flexbox .aplicacion-anuncio h2, .no-flexbox .producto-anuncio h2 {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  padding-top: 0.5rem;
  width: 270px;
  height: 1.15rem;
}
/*
  ----------------------------------------
  Galería Inicio
  ----------------------------------------
*/
.galeria {
  position: relative;
  width: auto;
  height: 85vh;
  margin: 0;
  text-align: center;
  border-bottom: 1px solid #eaeaea;
}
.galeria-carrusel {
  overflow: hidden;
  height: 100%;
}
.galeria-anuncio {
  width: 100%;
  height: 100%;
}
.galeria-anuncio h1 {
  font-weight: 300;
  font-size: 3.0rem;
  line-height: 3.5rem;
  margin-bottom: 1.0rem;
}
.galeria-anuncio p {
  font-size: 1.15rem;
  font-weight: 400;
  line-height: 1.75rem;
  letter-spacing: 0.025rem;
  margin-bottom: 1.5rem;
}
.galeria-anuncio .boton.accion {
  font-size: 1.0rem;
  padding: 1.25rem 1.0rem;
}
/* Cubierta - - - - - - - - - */
.anuncio-cubierta {
  height: 100%;
}
.js .anuncio-cubierta {
  display: flex;
  align-items: center;
  justify-content: center;
}
.no-js .anuncio-cubierta {
  position: relative;
}
.no-js .anuncio-cubierta .no-js {
  position: absolute;
  height: 200px;
  width: 315px;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
}
/* Detalle - - - - - - - - - */
.anuncio-detalle {
  position: relative;
  height: 100%;
  text-align: left;
}
.js .detalle-contenido {
  display: flex;
  align-items: center;
  padding: 0;
}
.no-js .detalle-contenido {
  padding: 25% 0 0 0;
}
.detalle-imagen {
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 0;
  max-width: 100%;
}
/*


  line 662

*/
<% data.site.index_page.each do |id, index| %>
  <% index.gallery.each do |ad| %>
    #<%= ad.title.parameterize %> {
      background-position: center;
      background-size: cover;
      background-repeat: no-repeat;
      background-image: url('<%= ad.background.url %>');
    }
  <% end %>
<% end %>
#google {
  padding: 0 0 25px;
  background-image:
   linear-gradient(rgba(255, 255, 255, 0.95), rgba(255, 255, 255, 0.10)),
   url('https://maps.googleapis.com/maps/api/staticmap?center=25.686260,-100.316536&zoom=11&size=640x640&scale=2&maptype=roadmap&style=feature:all|element:labels|visibility:off&key=AIzaSyCP_ev9agUDJGJCRu1haDffAFYAYLhV1Ug');
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
}
#google .contenedor-movil-carrusel {
  padding: 0 25px;
}
.contenedor-tarjetas-sucursal {
  padding: 0 0 50px 0;
}
.tarjeta-sucursal {
  display: inline-block;
  overflow: hidden;
  white-space: nowrap;
  min-width: 80%;
  margin: 0 12.5px;
  padding: 30px;
  text-align: left;
  text-decoration: none;
  border: 1px solid #eaeaea;
  background-color: white;
}
.tarjeta-sucursal .encabezado {
  margin-bottom: 2.0rem;
}
.tarjeta-sucursal h2 {
  display: block;
  margin-bottom: 0.5rem;
  font-weight: 400;
  font-size: 1.15rem;
  line-height: 1.15rem;
  letter-spacing: 0.025rem;
  text-transform: none;
  color: #212121;
}
.tarjeta-sucursal h4 {
  display: block;
  font-weight: 400;
  font-size: 0.75rem;
  line-height: 0.75rem;
  color: #b3b3b3;
}
.tarjeta-sucursal .encabezado {
  display: block;
  overflow: hidden;
  margin-bottom: 1.0rem;
}
.tarjeta-sucursal .direccion {
  margin: 1.0rem 0;
}
.esconder-movil, .esconder-movil-chico {
  display: none;
}
.sucursal-siguiente, .sucursal-previa {
  display: none;
}
/*
  ----------------------------------------
  Detalle de Productos por Aplicación
  ----------------------------------------
*/
.flexbox .portada h1 {
  max-width: 50%;
}
.no-flexbox .portada h1 {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  margin: auto;
  width: 270px;
  height: 60px;
  text-align: center;
}
.anuncio .contenido {
  overflow: hidden;
  padding-bottom: 0;
}
.anuncio .subcontenido {
  overflow: hidden;
}
.detalle-anuncio {
  padding: 40px 0 0 0;
}
.anuncio-barra {
  display: inline-block;
  width: 100%;
  margin: 0 auto;
  padding: 1.0rem 0;
}
.anuncio h1 {
  font-family: 'Open Sans', sans-serif;
  font-size: 2.0rem;
  margin: 0 0 1.0rem 0;
}
.anuncio-barra h4, .anuncio-barra p, .anuncio-barra a {
  display: inline-block;
  font-family: 'Open Sans', sans-serif;
  color: #212121;
}
.anuncio-barra h4 {
  padding: 0 3px 0 0;
  font-weight: 600;
  font-size: 1.0rem;
}
.anuncio-barra p {
  font-size: 0.875rem;
}
.anuncio p {
  margin: 0 0 1.0rem 0;
  font-size: 1.0rem;
  color: #969696;
}
.anuncio a {
  margin: 0 0 1.0rem 0;
  font-weight: 400;
  font-size: 1.0rem;
  color: #25c315;
}
/*
  ----------------------------------------
  Tarjeta de Producto
  ----------------------------------------
*/
.tarjeta-producto .enlace-imagen {
  overflow: -moz-scrollbars-none;
  display: inline-block;
  position: relative;
  width: 100%;
  height: 100%;
}
.tarjeta-producto .cubierta {
  display: block;
  position: absolute;
  opacity: 0;
  width: 100%;
  height: 100%;
  background-color: #000000;
}
.tarjeta-producto header {
  padding: 0.75rem 0 1.5rem 15px;
  text-align: left;
}
.tarjeta-producto header a {
  font-family: 'Open Sans', sans-serif;
  font-weight: 400;
  font-size: 1.0rem;
  letter-spacing: 0.01rem;
  line-height: 1.25rem;
  text-decoration: none;
  color: #212121;
}
.tarjeta-producto header span {
  display: inline-block;
  position: relative;
  top: -2px;
  margin: 0 0 0 5px;
  padding: 4px 8px;
  font-family: 'Open Sans', sans-serif;
  font-weight: 600;
  font-size: 0.5rem;
  text-transform: uppercase;
  letter-spacing: 0.1rem;
  border: 1px solid #f5a623;
  color: #f5a623;
}
/*
  ----------------------------------------
  Tarjeta de Servicio
  ----------------------------------------
*/
#servicios-relacionados {
  padding: 0 0 10px 0;
}
.tarjeta-servicio {
  display: inline-block;
  min-width: 275px;
  width: 60%;
  margin: 0 0 50px 25px;
  padding: 30px;
  white-space: normal;
  text-decoration: none;
  text-align: left;
  border: 1px solid #eaeaea;
  background-color: white;
}
.tarjeta-servicio:last-of-type {
  margin-right: 25px;
}
.tarjeta-servicio:active {
  background-color: #fbfbfb;
}
.tarjeta-servicio .enlace {
  margin-top: 1.0rem;
  color: #25c315;
}
.icono {
  display: inline-block;
  width: 30px; height: 30px;
  margin-bottom: 1.5rem;
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
}
.contenedor-movil-carrusel .tarjeta-producto header {
  padding: 0.75rem 0;
}
.contenedor-movil-carrusel .tarjeta-producto header h3 {
  margin-bottom: 7px;
}
/*
  ----------------------------------------
  Lista de Productos
  ----------------------------------------
*/
.portada-categoria {
  height: 350px;
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
}
<% data.site.categories.each do |id, category| %>
  #<%= category.name.parameterize %>-portada {
    background-image: url('<%= category.hero_image.url %>');
  }
<% end %>
/*
  ----------------------------------------
  Tarjeta de Información Nosotros
  ----------------------------------------
*/
.tarjeta-nosotros {
  padding: 0 2.0rem;
}
.tarjeta-nosotros .contenido {
  margin: 0 0 50px 0;
}
.tarjeta-nosotros .contenido.ultimo {
  margin: 0;
}
.nosotros-descripcion, .nosotros-conclusion {
  margin: 0 0 100px 0;
}
/*
  ----------------------------------------
  Empleos
  ----------------------------------------
*/
.empleos header {
  margin: 0 0 75px 0;
}
