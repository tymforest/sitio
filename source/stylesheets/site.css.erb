  color: #212121;
  background-color: #f0f0f0;
}
.boton-acordion {
  position: absolute;
  top: 0;
  right: 6.5px;
  height: 46px;
  width: 46px;
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
}
.abrir-acordion, .cerrar-acordion {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  height: 10px;
  width: 10px;
