}
.principal #enlaces, .principal #contacto {
  display: none;
}
/* Menu Interactivo - - - - - - - - - - */
#cubierta-menu {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 3;
  pointer-events: none;
  background-color: rgba(0,0,0,0);
  -webkit-transition: background-color 0.25s ease-in-out 0.10s;
  -moz-transition: background-color 0.25s ease-in-out 0.10s;
  -o-transition: background-color 0.25s ease-in-out 0.10s;
  transition: background-color 0.25s ease-in-out 0.10s;
}
#abrir-menu, #cerrar-menu {
  display: block;
  position: absolute;
  z-index: 2;
  top: 0;
  right: 0;
  width: 60px;
  height: 60px;
}
#abrir-menu {
  /* Agregar estilos.
  Por ejemplo...

  background-color: pink;
  border-left: 1px solid #eaeaea;

  */
}
.no-js #abrir-menu, .no-js #cerrar-menu {
  display: none;
}
#abrir-menu div, #cerrar-menu div {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  height: 15px;
  width: 15px;
  margin: auto;
  background-position: center;
  background-size: contain;
  background-repeat: no-repeat;
}
#abrir-menu div {
  background-image: url('../images/iconos/abrir-menu.svg');
}
#cerrar-menu div {
  background-image: url('../images/iconos/cerrar-menu.svg');
}
.no-js #menu {
  display: none;
}
#menu {
  position: fixed;
  z-index: 4;
  top: 0;
  right: -100%;
  width: 100%;
  height: 100%;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  background-color: #f6f6f6;
  border-left: 1px solid #eaeaea;
  border-bottom: none;
  -webkit-transition: right 0.25s cubic-bezier(0.45, 0.25, 0.25, 1) 0.20s;
  -o-transition: right 0.25s cubic-bezier(0.45, 0.25, 0.25, 1) 0.20s;
  -moz-transition: right 0.25s cubic-bezier(0.45, 0.25, 0.25, 1) 0.20s;
  transition: right 0.25s cubic-bezier(0.45, 0.25, 0.25, 1) 0.20s;
}
#menu::-webkit-scrollbar {
  display: none;
}
#menu header {
  width: 100%;
  height: 59px;
  padding: 11px 25px;
  background-color: white;
}
