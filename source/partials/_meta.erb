<% data.meta.favicons.each do |favicon| %>
  <% favicon.sizes.each do |size| %>
    <link rel="<%= favicon.rel %>" sizes="<%= size %>" href="images/metas/<%= favicon.rel %>-<%= size %><%= favicon.extension %>">
  <% end %>
<% end %>
<% data.meta.tiles.each do |meta| %>
  <meta name="<%= meta.name %>" content="<%= meta.content %>">
<% end %>
<!-- <%= data.meta.comments.favicons %> -->
<link rel="shortcut icon" type="image/x-icon" href="/favicon.ico">
<link rel="icon" type="image/x-icon" href="/favicon.ico">
